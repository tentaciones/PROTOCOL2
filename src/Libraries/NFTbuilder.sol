pragma solidity =0.8.7;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Base64.sol";

/// @title NFTSVG
/// @notice Provides a function for generating an SVG associated with a Nitro NFT
library NFTbuilder {
    using Strings for uint256;

    struct Data {
        string collateralFactor;
        string interestRate;
        string tokenId;
        string name;
        string pair;
    }

    function getmetadata(Data memory param)
        public
        pure
        returns (string memory)
    {
        string memory base = "data:application/json;base64,";
        string memory metadata = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{ "name": "',
                        param.name,
                        '","description":"This NFT is a representation of a Liquidity Position in Nitro Finance",',
                        '"image":"',
                        generateSVG(
                            param.tokenId,
                            param.pair,
                            param.collateralFactor,
                            param.interestRate
                        ),
                        '"}'
                    )
                )
            )
        );

        return string(abi.encodePacked(base, metadata));
    }

    function generateSVG(
        string memory tokenId,
        string memory pair,
        string memory col,
        string memory interest
    ) public pure returns (string memory svg) {
        string memory BASEURL = "data:image/svg+xml;base64,";
        return
            string(
                abi.encodePacked(
                    BASEURL,
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                generategraphUpper(col, interest),
                                generategraphLower(col, interest, tokenId),
                                generateInfo(col, interest, pair)
                            )
                        )
                    )
                )
            );
    }

